{% block scripts %}
	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
	<script>
		$(document).ready(function() {
			$('.details').toggle();
		
		
			$('.reveal').click(function(e){ 
				e.preventDefault();
				$(this).closest(".scheduled-event").find(".details").slideToggle("fast"); 
			})
			
			$(document).keyup(function(e) {
				if (e.keyCode == 27)  { $('.the-day').find(".details:visible").slideToggle("fast");  }  // esc 
    		});
		});
	</script>
{% endblock %}